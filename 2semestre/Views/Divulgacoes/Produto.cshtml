        </center>

        <div id="botoes-container">
            <img class="curtirFavoritar curso-pointer zoom-in" src="~/img/coracao.png" onclick="toggleText()">
            <button id="botao-center" class="btn btn-primary curso-pointer" type="submit">Saiba mais</button>
            <img class="curtirFavoritar curso-pointer zoom-in" src="~/img/favorito.png" onclick="toggleText()">

        </div>
    </div>
</section>


